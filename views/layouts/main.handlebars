<!doctype html>
<html lang="en">
  <head>
    <title>Forward Line</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="css/open-iconic-master/font/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="../css/fontawesome/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="../css/style.css">
  </head>
  <body>

    <div class="jumbotron">
        <div class="container">
            <div class="row">
                <div class="col-6"><h1><img id="fl-logo" src="../img/fl-logo.svg" alt=""></h1></div>
                <div class="col-6"><span class="cta">call us: <a href="tel:8887334889">888.733.4889</a></span></div>
            </div>
            <div class="row">
                <div class="col-12">
                    {{> page/nav}}
                </div>
            </div>
        </div>
    </div>

      
    <div id="main" class="container mb-5">
        <div class="row">
            {{{body}}}
        </div>
        <div class="row mt-4">
            <div class="col-sm-6">
                <form>
                    <button onclick="(window.location.href = '{{nextStep}}')" id="continue" class="btn btn-primary btn-lg">{{continueButton}}</button>
                </form>
            </div>
            <div class="col-sm-3"><span class="mssgOutput"></span></div>
        </div>
    </div>

    

    <footer class="sticky-footer">
        {{> page/secure}}
    </footer>

    
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script
        src="https://code.jquery.com/jquery-3.3.1.min.js"
        integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="../js/jquery.inputmask.bundle.min.js"></script>
    <script src="../js/script.js"></script>
  </body>
</html>